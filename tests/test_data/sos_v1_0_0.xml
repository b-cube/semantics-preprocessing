    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <!-- operations Metadata Section -->
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <ows:OperationsMetadata>
        <ows:Operation name='GetCapabilities'>
            <ows:DCP>
                <ows:HTTP>
                   <ows:Get xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp'/>
                   <ows:Post xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp'/>
                </ows:HTTP>
            </ows:DCP>
            <ows:Parameter name='service'>
                <ows:AllowedValues>
                    <ows:Value>SOS</ows:Value>
                </ows:AllowedValues>
            </ows:Parameter>
            <ows:Parameter name='version'>
                <ows:AllowedValues>
                    <ows:Value>1.0.0</ows:Value>
                </ows:AllowedValues>
           </ows:Parameter>
        </ows:Operation>
        <ows:Operation name='GetObservation'>
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp'/>
                    <ows:Post xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp'/>
                </ows:HTTP>
            </ows:DCP>
            <ows:Parameter name='offering'>
                <ows:AllowedValues>
					<ows:Value>101</ows:Value>
					<ows:Value>102</ows:Value>
					<ows:Value>103</ows:Value>
					<ows:Value>104</ows:Value>
					<ows:Value>105</ows:Value>
                </ows:AllowedValues>
            </ows:Parameter>
            <ows:Parameter name='eventTime'>
                <ows:AnyValue/>
            </ows:Parameter>
            <ows:Parameter name='observedProperty'>
                <ows:AllowedValues>
					<ows:Value>air_pressure</ows:Value>
					<ows:Value>air_temperature</ows:Value>
					<ows:Value>dissolved_oxygen</ows:Value>
					<ows:Value>relative_humidity</ows:Value>
					<ows:Value>salinity</ows:Value>
					<ows:Value>downward_shortwave_radiation</ows:Value>
					<ows:Value>surface_elevation</ows:Value>
					<ows:Value>water_temperature</ows:Value>
					<ows:Value>wind_speed</ows:Value>
                </ows:AllowedValues>
            </ows:Parameter>
        </ows:Operation>
        <ows:Operation name='DescribeSensor'>
        	<ows:DCP>
        		<ows:HTTP>
        			<ows:Get xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp' /> 
        			<ows:Post xlink:href='http://weather.lumcon.edu/sos/oostethys_server.asp' /> 
        		</ows:HTTP>
        	</ows:DCP>
        	<ows:Parameter name='procedure'>
        		<ows:AllowedValues>
         			<ows:Value>urn:disl.org:source.mooring#disl</ows:Value> 
         			<ows:Value>urn:disl.org:source.mooring#mbla</ows:Value> 
         			<ows:Value>urn:disl.org:source.mooring#mhpa</ows:Value> 
         		</ows:AllowedValues>
          	</ows:Parameter>
         </ows:Operation>
    </ows:OperationsMetadata>
